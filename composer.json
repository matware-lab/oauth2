{
    "name": "matware-lab/oauth2",
    "type": "joomla-package",
    "description": "Joomla OAuth2 Server Package",
    "keywords": ["joomla", "framework", "oauth2", "server", "plugin"],
    "homepage": "https://github.com/matware-lab/oauth2",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^5.3.10|~7.0",
        "joomla/application": "~1.0|~2.0",
        "joomla/http": "^1.2.2|~2.0",
        "joomla/input": "~1.2|~2.0",
        "joomla/session": "~1.0|~2.0",
        "joomla/date": "^1.2",
        "joomla/controller": "~2.0@dev"
    },
    "require-dev": {
        "joomla/coding-standards": "~2.0@alpha",
        "phpunit/phpunit": "^4.8.35|^5.4.3|~6.0"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\OAuth2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\OAuth2\\Tests\\": "Tests/"
        }
    }
}
